{
  "name": "fintwit-performance",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "server": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run web\"",
    "webhooks": "./stripe.exe listen --forward-to localhost:3000/api/stripe/webhook",
    "dev:full": "concurrently \"npm run server\" \"npm run webhooks\" \"npx expo start --web --port 8083\"",
    "sim:extract": "node simulation/extract.js && node simulation/normalize.js",
    "sim:load": "node simulation/load.js",
    "sim:all": "npm run sim:extract && npm run sim:load",
    "api:dev": "uvicorn main:app --host 0.0.0.0 --port 8000 --reload --app-dir server"
  },
  "dependencies": {
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.8",
    "@supabase/supabase-js": "^2.58.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.6.1",
    "expo": "~54.0.10",
    "expo-linear-gradient": "~15.0.7",
    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.8",
    "express": "^4.21.2",
    "micro": "^10.0.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "stripe": "^19.0.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "private": true
}
